{
    "manifest_version": 2,

    "name": "edunga-scrapper",
    "description": "smart scrapper",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click here!",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}